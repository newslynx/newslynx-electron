{
  "name": "newslynx-electron",
  "productName": "Newslynx",
  "version": "0.1.0",
  "description": "A desktop version of the NewsLynx web app.",
  "main": "index.js",
  "repository": "newslynx/newslynx-electron",
  "author": {
    "name": "mhkeller",
    "email": "michael.keller@gmail.com",
    "url": "newslynx.org"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . NewsLynx --prune --asar --overwrite --platform=darwin --arch=x64 --version=0.28.1 --icon='assets/merlynne-ears.icns' --ignore='node_modules/(electron-packager|electron-prebuilt)'"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "electron-debug": "^0.1.0",
    "isotope-layout": "^2.2.1",
    "jquery": "^1.11.0",
    "moment": "^2.8.3",
    "newslynx": "^0.1.0"
  },
  "devDependencies": {
    "electron-packager": "5.0.0",
    "electron-prebuilt": "^0.28.1"
  }
}
